# Dash
PORT=8050
LOG_LEVEL=INFO
ENABLE_SCHEDULER=true
DEFAULT_MONTHS=3

# Supabase
SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
# Use Service Role key for server-side ingestion/upserts.
SUPABASE_SERVICE_ROLE_KEY=YOUR_SERVICE_ROLE_KEY
SUPABASE_SCHEMA=public

# Optional: identify instance (otherwise auto-generated)
INSTANCE_ID=
INGEST_LOCK_NAME=global_ingestion_lock
INGEST_LOCK_LEASE_SECONDS=120
